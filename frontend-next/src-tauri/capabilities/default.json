{
  "identifier": "default",
  "description": "Default capabilities for TranscribeAlpha desktop",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "fs:default",
    "fs:read-all",
    "fs:write-all",
    "fs:allow-exists",
    "fs:allow-mkdir",
    "fs:allow-remove",
    "fs:allow-read-dir",
    "fs:allow-rename",
    "fs:allow-copy-file",
    "fs:allow-stat",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "dialog:allow-ask",
    "store:default",
    "shell:default",
    "shell:allow-open",
    "shell:allow-execute",
    "shell:allow-spawn",
    "updater:default"
  ]
}
